    "''' \n",
    "Arithemetic Operators \n",
    "+ = Addition\n",
    "- = Subtraction \n",
    "/ = Division\n",
    "* = Multiplication\n",
    "% = Modulus; Returns Remainder (Number remains after division)\n",
    "// = Floor Division; Returns Quotient  \n",
    "** = Exponentiation; Returns Square \n",
    "'''\n",
    "\n",
    "# NUMBER INPUT \n",
    "print(\"\\nPlease Enter:\")\n",
    "\n",
    "number1 = int(input(\"Number 1: \"))\n",
    "number2 = int(input(\"Number 2: \"))\n",
    "\n",
    "\n",
    "# Menu\n",
    "print(\"\"\"\\n1. Addition \\n2. Subtraction \\n3. Multiplication \\n4. Division \\n5. Modulo \\n6. Floor Division \\n7. Exponentiation\"\"\")\n",
    "\n",
    "x = int(input(\"\\nPlease Enter Your Selection from 1 to 7: \"))\n",
    "\n",
    "# Menu-driven Program\n",
    "if(x==1):\n",
    "    print(\"\\n\\n\", \"Addition: \\n\", number1, \"+\", number2,\"=\",number1 + number2)\n",
    "\n",
    "if(x==2):\n",
    "    print(\"\\n\\n\", \"Subtraction: \\n\", number1, \"-\", number2,\"=\", number1 - number2)\n",
    "\n",
    "if(x==3):\n",
    "    print(\"\\n\\n\", \"multiplication: \\n\", number1, \"*\", number2,\"=\", number1 * number2)\n",
    "\n",
    "if(x==4):\n",
    "    print(\"\\n\\n\", \"Division: \\n\", number1, \"/\", number2,\"=\", number1 / number2)\n",
    "\n",
    "if(x==5):\n",
    "    print(\"\\n\\n\", \"Modulo: \\n\", number1, \"%\", number2,\"=\", number1 % number2)\n",
    "\n",
    "if(x==6):\n",
    "    print(\"\\n\\n\", \"Floor Division: \\n\", number1, \"//\", number2,\"=\", number1 // number2)\n",
    "\n",
    "if(x==7):\n",
    "    print(\"\\n\\n\", \"Exponentiation: \\n\", number1, \"**\", number2,\"=\", number1 ** number2)\n",
    " \n"
