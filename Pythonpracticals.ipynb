    "# DICTIONARY FUNCTIONS AND MANIPULATIONS\n",
    "\n",
    "def print_items(dict1):    \n",
    "    print(\"Dictionary Items\")\n",
    "    for i in dict1.items():\n",
    "        print(i[0],\" - \" ,i[1])\n",
    "\n",
    "dict1 = {1:'one', 2:'two', 3:'three', 4:'four', 5:'five'}\n",
    "\n",
    "while True:\n",
    "    # MENU\n",
    "    print(\"Dictionary Manipulation\")\n",
    "    print(\"1. Add Key:Value pair\")\n",
    "    print(\"2. Remove Key:Value pair\")\n",
    "    print(\"3. Update value of a key\")\n",
    "    print(\"4. Check if key exists\")\n",
    "    print(\"5. Get value of a key\")\n",
    "    print(\"6. Get all Keys\")\n",
    "    print(\"7. Get all Values\")\n",
    "    print(\"8. Print Dictionary\")\n",
    "    print(\"9. EXIT\")\n",
    "\n",
    "    choice = int(input(\"Please select from list (1-9)\"))\n",
    "\n",
    "    if choice == 1:\n",
    "        key = input(\"Enter Key: \")\n",
    "        value = input(\"Enter Value: \")\n",
    "        dict1[key] = value\n",
    "        print_items(dict1)\n",
    "\n",
    "    elif choice == 2:\n",
    "        key = input(\"Enter Key to remove: \")\n",
    "        if key in dict1:\n",
    "            del dict1[key]\n",
    "            print_items(dict1)\n",
    "        else:\n",
    "            print(f\"Key {key} not found\")\n",
    "    \n",
    "    elif choice == 3:\n",
    "        key = input(\"Enter Key to update: \")\n",
    "        if key in dict1:\n",
    "            value = (\"Enter new Value: \")\n",
    "            dict1[key] = value\n",
    "            print_items(dict1)\n",
    "        else:\n",
    "            print(f\"Key {key} not found\")\n",
    "\n",
    "    elif choice == 4:\n",
    "       key = input(\"Enter Key to check: \")\n",
    "       if key in dict1:\n",
    "           print(f\"Key {key} found\") \n",
    "       else:\n",
    "           print(f\"Key {key} not found\")\n",
    "    \n",
    "    elif choice == 5:\n",
    "        key = input(\"Enter Key to get Value\")\n",
    "        if key in dict1:\n",
    "            value = dict1[key]\n",
    "            print(f\"Value of {key} is {value}\")\n",
    "        else:\n",
    "            print(f\"Key {key} not found\")\n",
    "\n",
    "    elif choice == 6:\n",
    "        keys = list(dict1.keys())\n",
    "        print(f\"All keys: {keys}\")\n",
    "\n",
    "    elif choice == 7:\n",
    "        values = list(dict1.values())\n",
    "        print(f\"All keys: {values}\")\n",
    "\n",
    "    elif choice == 8:\n",
    "        print_items(dict1)\n",
    "\n",
    "    elif choice == 9:\n",
    "        print(\"EXITED\")\n",
    "        break\n",
    "\n",
    "    else:\n",
    "        print(\"Invalid Choice.\")\n"
