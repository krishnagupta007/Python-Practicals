    "    lst.append(ele)\n",
    "    print(f\"Element successfully added\")\n",
    "\n",
    "def update(lst, old_ele, new_ele):\n",
    "    if old_ele in lst:\n",
    "        index = lst.index(old_ele)\n",
    "        lst[index] = new_ele\n",
    "        print(f\"{old_ele} replaced with {new_ele}\")\n",
    "    else:\n",
    "        print(f\"{old_ele} not exists in list\")\n",
    "\n",
    "def remove(lst,ele):\n",
    "    if ele in lst:\n",
    "        lst.remove(ele)\n",
    "        print(f\"{ele} successfully removed\")\n",
    "    else:\n",
    "        print(f\"{ele} not exists in list\")\n",
    "\n",
    "def traverse_lr(lst):\n",
    "    print(\"TRAVERSING \\nLEFT TO RIGHT\") \n",
    "    for ele in lst:\n",
    "        print(ele)\n",
    "\n",
    "def traverse_rl(lst):\n",
    "    print(\"TRAVERSING \\nRIGHT TO LEFT\")\n",
    "    for ele in lst[::-1]:\n",
    "        print(ele)\n",
    "\n",
